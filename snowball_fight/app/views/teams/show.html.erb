<h2><%= @team.name %></h2>

<p><strong>League: </strong> <%= link_to @team.league.name, @team.league %></p>
<p><strong>Players: </strong></p>
<ul>
  <% @team.players.each do |player| %>
    <li><%= link_to player.name, player %></li>
  <% end %>
</ul>

<%= link_to "Add Players To Your Team", players_path %>
